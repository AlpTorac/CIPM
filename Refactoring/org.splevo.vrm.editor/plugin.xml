<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated vrm -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.splevo.vrm.presentation.vrmModelWizardID"
            name="%_UI_vrmModelWizard_label"
            class="org.splevo.vrm.presentation.vrmModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/vrmModelFile.gif">
         <description>%_UI_vrmModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated vrm -->
      <editor
            id="org.splevo.vrm.presentation.vrmEditorID"
            name="%_UI_vrmEditor_label"
            icon="icons/full/obj16/vrmModelFile.gif"
            extensions="vrm"
            class="org.splevo.vrm.presentation.vrmEditor"
            contributorClass="org.splevo.vrm.presentation.vrmActionBarContributor">
      </editor>
   </extension>

</plugin>
